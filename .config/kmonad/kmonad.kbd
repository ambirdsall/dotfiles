(defcfg
  input (device-file "/dev/input/by-id/usb-Apple_Inc._Apple_Internal_Keyboard___Trackpad_D3H5116FJ71GHJGAS6LS-if01-event-kbd")
  output (uinput-sink "My KMonad output")
  fallthrough true)

(defsrc
  caps lsft rsft sys)

(deflayer qwerty
  @cap @lshft @rshft rmeta)

(defalias
  cap   (tap-hold-next 333 esc lctl)
  lshft (tap-hold-next 333 S-9 lsft)
  rshft (tap-hold-next 333 S-0 rsft))
