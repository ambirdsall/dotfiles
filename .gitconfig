[user]
	name = Alex Birdsall
	email = ambirdsall@gmail.com
[alias]
	c = "!_f(){ [ $# -gt 0 ] && git commit -m \"$*\" || git commit -v; };_f"
	co = checkout
	d = diff
	dc = diff --cached
	p = add -p
	latest = "!git reflog | grep checkout: | awk '{print $6}' | awk '!H[$0]++'"
	shove = push --force-with-lease
	l = log --oneline
[rerere]
	enabled = true
[github]
	user = ambirdsall
[init]
	defaultBranch = main
